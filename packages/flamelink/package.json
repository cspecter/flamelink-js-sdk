{
  "name": "flamelink",
  "version": "0.0.1",
  "description": "Universal Javascript SDK for integrating with your Flamelink CMS",
  "author": "Flamelink <feedback@flamelink.io> (https://flamelink.io)",
  "homepage": "https://flamelink.github.io/flamelink-js-sdk/",
  "license": "MIT",
  "keywords": [
    "flamelink",
    "firelink",
    "firebase",
    "firestore",
    "headless",
    "cms",
    "javascript",
    "sdk",
    "web",
    "node",
    "caas",
    "real-time"
  ],
  "files": [
    "app",
    "content",
    "dist",
    "navigation",
    "schemas",
    "settings",
    "storage",
    "users",
    "/public.d.ts",
    "/flamelink*.js",
    "/flamelink*.br",
    "/flamelink*.gz",
    "/flamelink*.map"
  ],
  "engines": {
    "node": ">=6.0.0"
  },
  "main": "dist/index.node.cjs.js",
  "browser": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "public.d.ts",
  "dependencies": {
    "@flamelink/sdk-app-types": "0.0.1",
    "@flamelink/sdk-app": "0.0.1",
    "@flamelink/sdk-schemas": "0.0.1",
    "@flamelink/sdk-content": "0.0.1",
    "@flamelink/sdk-settings": "0.0.1",
    "@flamelink/sdk-storage": "0.0.1",
    "@flamelink/sdk-navigation": "0.0.1",
    "@flamelink/sdk-users": "0.0.1",
    "compose-then": "^1.0.1"
  },
  "scripts": {
    "prebuild": "rimraf dist **/dist flamelink-* flamelink.*",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest"
  }
}
